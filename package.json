{
  "name": "kevast",
  "version": "0.1.0",
  "description": "Kevast for browser and Node.js. One key-value interface, all kinds of storages.",
  "main": "dist/index.js",
  "keywords": [
    "kevast",
    "key",
    "value",
    "key-value",
    "interface",
    "storage",
    "redundancy",
    "middleware"
  ],
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "lint": "tslint -p tsconfig.json",
    "test": "npm run test:nodejs && npm run test:browser",
    "build": "npm run build:nodejs && npm run build:browser",
    "prebuild": "npm run clean",
    "build:nodejs": "tsc",
    "build:browser": "webpack",
    "pretest:browser": "node ./test/browser/build.js",
    "test:nodejs": "mocha --require espower-typescript/guess \"test/**/*.ts\"",
    "test:browser": "open ./test/browser/test.html",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true,
  "repository": "git@github.com:kevast/kevast.js.git",
  "author": "Andie <zchangan@163.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.15",
    "babel-loader": "^8.0.4",
    "espower-typescript": "^9.0.1",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.1",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  }
}
